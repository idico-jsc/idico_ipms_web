{
  "name": "parent-portal",
  "private": true,
  "version": "0.0.100",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "generate:routes": "node scripts/generate-routes.js",
    "test": "cypress run",
    "test:headless": "start-server-and-test dev http://localhost:5173 'cypress run'",
    "e2e": "start-server-and-test dev http://localhost:5173 cypress",
    "build:mobile": "npm run build && npm run setup:android && npx cap sync && npm run copy:config",
    "build:apk:debug": "npm run build:mobile && cd build/android && ./gradlew assembleDebug && cd ../.. && node scripts/rename-apk.js debug",
    "build:apk:release": "npm run build:mobile && cd build/android && ./gradlew assembleRelease && cd ../.. && node scripts/rename-apk.js release",
    "setup:android": "node scripts/setup-android-resources.js",
    "copy:config": "node scripts/copy-config-files.cjs",
    "cap:sync": "npm run setup:android && npx cap sync && npm run copy:config",
    "cap:sync:android": "npm run setup:android && npx cap sync android && npm run copy:config",
    "cap:open:android": "npx cap open android",
    "cap:run:android": "npx cap run android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-oauth/google": "^0.12.2",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^12.6.0",
    "frappe-react-sdk": "^1.13.0",
    "html-react-parser": "^5.2.8",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.561.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^16.0.1",
    "react-router": "^7.8.0",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "virtual": "^0.2.0",
    "vite-plugin-svgr": "^4.3.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.8",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@testing-library/cypress": "^10.1.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.6.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/tailwindcss": "^3.1.0",
    "@types/zxcvbn": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^15.4.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8.5.6",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "start-server-and-test": "^2.1.2",
    "tailwindcss": "^4.1.14",
    "terser": "^5.44.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-fonts": "^1.4.0",
    "unplugin-imagemin": "^0.6.9",
    "vite": "^7.1.2",
    "vite-plugin-compression2": "^2.2.1",
    "vite-plugin-inspect": "^11.3.2",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.3.0"
  }
}
